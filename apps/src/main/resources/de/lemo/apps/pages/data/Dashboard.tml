<div class="modal hide fade" id="widgetDialog">
				<div class="modal-header">
					<a href="#" class="close" data-dismiss="modal">&times;</a>
					<h3>${message:widgetPrefs}</h3>
					</div>
				<div class="modal-body">
					<div class="divDialogElements">
						<input class="xlarge" id="xlInput" name="xlInput" type="text" />
						</div>
					</div>
				<div class="modal-footer">
					<a href="#" class="btn" onclick="closeDialog ();">Cancel</a>
					<a href="#" class="btn btn-primary" onclick="okClicked ();">OK</a>
				</div>
</div>
			     
     
 <script>
			$(document).ready(function() {
				$('#widgetDialog').bind('show', function () {
					document.getElementById("xlInput").value = "widget3";
					});
				});
			function closeDialog () {
				$('#widgetDialog').modal('hide'); 
				};
			function okClicked () {
				closeDialog ();
				};
</script>    
     
