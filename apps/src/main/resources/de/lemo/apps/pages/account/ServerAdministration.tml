<html t:type="layoutaccount" title="LeMo | ${message:myAccountTitle}" subpage="serveradministration"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
	
		
		<div class="span9">
			<div class="row-fluid">
				<div class="span12">
					<h3>DM-Server ${message:administration}</h3>
					<br />
				</div>
			</div>
			<div class="row-fluid">
				<div class="span12">	    	
					   	
					<form t:id="serverform" class="well  pull-left">
				    	${message:serverstatus}:
				    	<t:if test="serverOnline">
				    		<span class="label label-success">Online</span>
				    	 	<p:else>
                				<span class="label label-important">Offline</span>
            				</p:else>
        				</t:if>
        				<br />
        				<br />
        				${message:serverStartTime}: ${starttime}
		            	
				    </form>	
			 	
			 	</div>  
			 </div> 
		</div>

</html>