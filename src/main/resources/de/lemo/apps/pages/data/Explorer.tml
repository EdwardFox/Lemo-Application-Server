<html t:type="layoutdashboard" title="LeMo | ${message:explorerTitle}"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
     
     <!-- Breadcrumb component-->
	<div class="row-fluid">
        <div class="span12">
    	<ul class="breadcrumb">
  			<li t:type="crumb/breadcrumbtrail" t:value="breadCrumb">
    			<a t:type="crumb/displaybreadcrumb" t:breadcrumb="breadCrumb" />
  			</li>
		</ul>
		</div>
	</div>
	
	<div class="row-fluid">
		<div class="span3">
			<div class="thumbnail">
    		
				<t:grid class="table" source="courses" model="coursesGridModel" row="course" rowsPerPage="12" t:inPlace="true">
		            <p:courseNameCell>
		            	<t:actionlink t:id="show" context="course.id" zone="coursezone">${course?.courseDescription}</t:actionlink>		                
		            </p:courseNameCell>
<!--		            <p:compareCell>-->
<!--		            	<label class="checkbox">-->
<!--		            	<form t:type="form">-->
							<t:actionlink t:id="compare" context="${course?.id}"><input t:type="checkbox" value="false"/></t:actionlink>
<!--							<input t:type="checkbox" value="false"/>-->
<!--						</form>-->
<!--						</label>-->
<!--		            </p:compareCell>-->
		            <p:empty>
		              <p>${message:noCourses}</p>
		            </p:empty>
        		</t:grid>
        		<t:form class="quickSearch">
        		<div class="">
        		
        			<div class="input-append">
						      <input t:type="TextField" t:id="courseSearch" t:mixins="jquery/Autocomplete" class="pull-left " placeholder="Course name"></input>
						      <span class="add-on"><a t:type="LinkSubmit" t:id="quickSearch" href="#"><i class="icon-search icon-black"></i></a></span>
					</div>
    				<br />
    			</div>
    			</t:form>
    		</div>
    	</div>
    	
    	
    	
    	<div class="span9">
    		<div class="thumbnail">
    		<div class="row-fluid">
	    		<div class="span12">
	    		    <ul id="summaryTab" class="nav nav-tabs">
	    		    
					    <li class="active"><a href="#summary" data-toggle="tab">${message:courseSummary}</a></li>
					    <li><a href="#lastmonth" data-toggle="tab">Last month</a></li>
					    <li><a href="#overall" data-toggle="tab">Overall</a></li>
					    <li><a href="#settings" data-toggle="tab">Settings</a></li>
					</ul>
				</div>
			</div>
    		<div> <!-- well content  -->
    		
				<div class="tab-content">
				    <div class="tab-pane active" id="summary">
						<div class="row-fluid">
						<div class="span1" />
							
						<div class="span5">
							<t:zone t:id="courseZone" id="coursezone" t:update="show"> 	
							<section>
								<header>
									<h3>${CurrentCourse?.courseDescription} (${CurrentCourse?.courseId})</h3><br />
								</header>
								<p><b>${message:firstRequest}:</b> ${firstRequestDate} <br /> </p>
								<p><b>${message:lastRequest}:</b> ${lastRequestDate} <br /> </p>
								<p><b>${message:courseEnroledParticipants}:</b> ${CurrentCourse?.enroledParticipants}<br /> </p> 
								
								<label class="checkbox">
		              <form t:type="form">
                    ${message:favoriteOption}
                    <t:actionlink t:id="favorite" context="${CurrentCourse?.id}">
                        <input t:type="checkbox" t:value="${favorite}"/>
                    </t:actionlink>
								  </form>
								</label>
								<p><br /><br /><br /></p>
								
								<t:pagelink page="viz/visualizationNVD3" id="${message:viewdetails}" context="${CurrentCourse?.id}" class="btn btn-primary pull-left">${message:viewdetails}</t:pagelink>
								
							</section>	
							</t:zone> <!-- courseZone  -->
						</div> <!-- span5 -->
						
						<div class="span6">
							<t:zone t:id="courseVisZone" id="courseVisZone" t:update="show"> 
								<div t:id="chart1" style="width: 450px; height: 250px;"/>
									
								<br /><br />
							</t:zone> <!-- courseVisZone  -->		
						</div> <!-- span6 -->
						
					</div>
					</div><!-- tab-pane summary -->
				    
				    <div class="tab-pane" id="lastmonth">
						<div class="row-fluid">
						<div class="span1" />
							
						<div class="span5">
							<t:zone t:id="courseLastMonthZone" id="courseLastMonthZone" t:update="show"> 	
							<section>
								<header>
									<h3>${CurrentCourse?.courseDescription} (${CurrentCourse?.courseId})</h3><br />
								</header>
								<p><b>${message:lastRequest}:</b> ${getLocalizedDateTime(CurrentCourse?.lastRequestDate)} <br /> </p>
		    					<br />
		    					<p><b>${message:overallRequests}: </b> ${getOverallRequest(usageAnalysisLastMonth)}<br /> </p> 
		    					<br />
		    					<p><b>${message:avgRequests}:</b> ${getAverageRequest(usageAnalysisLastMonth)}<br /> </p> 
		    					<br />
		    					<p><b>${message:mostRequests}:</b> ${getLocalizedDate(getMaxRequestDate(usageAnalysisLastMonth))}  - (${getMaxRequest(usageAnalysisLastMonth)})<br /> </p> 	
		    					<br />
								<br />
							</section>	
							</t:zone> <!-- courseLastMonthZone  -->
						</div> <!-- span5 -->
						
						<div class="span5">
							<t:zone t:id="courseVisLastMonthZone" id="courseVisLastMonthZone" t:update="show"> 
<!--								<div t:id="chart2" style="width: auto; height: 300px;"/>-->
							</t:zone> <!-- courseVisZone  -->		
						</div> <!-- span5 -->
						
					</div><!-- tab-pane  last month -->
					
					</div>
				    <div class="tab-pane" id="overall">
				    	<div class="row-fluid">
						<div class="span1" />
							
						<div class="span5">
							<t:zone t:id="courseOverallZone" id="courseOverallZone" t:update="show"> 	
							<section>
								<header>
									<h3>${CurrentCourse?.courseDescription} (${CurrentCourse?.courseId})</h3><br />
								</header>
								<p><b>${message:firstRequest}:</b> ${getLocalizedDateTime(CurrentCourse?.firstRequestDate)} <br /> </p>
		    					<br />
		    					<p><b>${message:overallRequests}: </b> ${getOverallRequest(usageAnalysisOverall)}<br /> </p> 
		    					<br />
		    					<p><b>${message:avgRequests}:</b> ${getAverageRequest(usageAnalysisOverall)}<br /> </p> 
		    					<br />
		    					<p><b>${message:mostRequests}:</b> ${getLocalizedDate(getMaxRequestDate(usageAnalysisOverall))}  - (${getMaxRequest(usageAnalysisOverall)})<br /> </p> 	
		    					<br />
								<br />
							</section>	
							</t:zone> <!-- courseLastMonthZone  -->
						</div> <!-- span5 -->
						
						<div class="span5">
							<t:zone t:id="courseVisOverallZone" id="courseVisOverallZone" t:update="show"> 
<!--								<div t:id="chart2" style="width: auto; height: 300px;"/>-->
							</t:zone> <!-- courseVisZone  -->		
						</div> <!-- span5 -->
						
					</div><!-- tab-pane  overall -->
				    </div>
				    <div class="tab-pane" id="settings">${message:notYetImplemented}</div>
				</div> <!-- tab-content -->
				
    		</div> <!-- class=well -->
    		</div> <!-- thumbnail -->
    		
    		<div class="row-fluid">
    			<div class="thumbnail">
    				<div class="row-fluid">
    					<div class="span1">
	    				</div>
	    				<div class="span11">
	    					<br />
	    					<b>${message:chooseAnalysisType}</b>
    						<br />
    						<br />
	    				</div>
    					
    				</div>
    				<t:zone t:id="analysisSelectZone" id="analysisSelectZone" t:update="show"> 
							
		    			<div class="row-fluid">
		    				
		    				<div class="span1">
		    				</div>
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationNVD3" context="${CurrentCourse?.id}"><img src="${usageAnalysisIcon}" height="50px" width="75px" /><br /><br /><b>${message:usageAnalysisTime}</b> </t:pagelink> 
			    				<br /><br />
			    			</div>
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationLONVD3" context="${CurrentCourse?.id}"><img src="${usageAnalysisLOIcon}" height="50px" width="75px" /><br /><br /><b>${message:usageAnalysisLO}</b> </t:pagelink> 
			    				<br /><br />
			    			</div>
			    			
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationLOTreeMap" context="${CurrentCourse?.id}"><img src="${usageAnalysisLOTreeMapIcon}" height="50px" width="75px" /><br /><br /><b>${message:usageAnalysisLOTreemap}</b> </t:pagelink> 
			    				<br /><br />
			    			</div>
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/activitygraph" context="${CurrentCourse?.id}"><img src="${navAnalysisIcon}" height="50px" width="85px" /><br /><br /><b>${message:navAnalysis}</b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/activitygraph_2" context="${CurrentCourse?.id}"><img src="${navAnalysisIcon}" height="50px" width="85px" /><br /><br /><b>${message:navAnalysis} 2</b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			
			    			
<!--			    			<div class="span2" align="center">-->
<!--			    				<t:pagelink page="data/visualizationD3_2" context="${CurrentCourse.id}"><img src="${pathAnalysisIcon}" height="50px" width="85px" /><br /><br /><b>Frequent ${message:pathAnalysis} 2</b> </t:pagelink>-->
<!--			    				<br /><br />-->
<!--			    			</div>-->
			    			<div class="span1">
			    			</div>	
			    		</div>
			    		<div class="row-fluid">
		    				
		    				<div class="span1">
		    				</div>
		    				<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationFP" context="${CurrentCourse?.id}"><img src="${pathAnalysisIcon}" height="50px" width="85px" /><br /><br /><b>${message:freqPathAnalysis} </b> </t:pagelink>
			    				<br /><br />
			    			</div>
		    				
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationFPViger" context="${CurrentCourse?.id}"><img src="${pathAnalysisIcon}" height="50px" width="85px" /><br /><br /><b>${message:freqPathAnalysis} Viger </b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			
			    			
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationPerformanceHistogram" context="${CurrentCourse?.id}"><img src="${usageAnalysisLOIcon}" height="50px" width="85px" /><br /><br /><b>Performance Histogram</b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationPerformanceHistogramAvg" context="${CurrentCourse?.id}"><img src="${usageAnalysisLOIcon}" height="50px" width="85px" /><br /><br /><b>Performance Histogram Avg</b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationPerformanceCumulative" context="${CurrentCourse?.id}"><img src="${cumulativeUserAnalysisIcon}" height="50px" width="85px" /><br /><br /><b>Performance Cumulative</b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			
			    			<div class="span1">
			    			</div>	
			    		</div>
			    		<div class="row-fluid">
		    				
		    				<div class="span1">
		    				</div>
			    			
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationPerformanceUserTestCumulative" context="${CurrentCourse?.id}"><img src="${cumulativeUserAnalysisIcon}" height="50px" width="85px" /><br /><br /><b>Performance User Test</b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/activityTimeCumulative" context="${CurrentCourse?.id}"><img src="${cumulativeUserAnalysisIcon}" height="50px" width="75px" /><br /><br /><b>${message:usageAnalysisTime} Cumulative</b> </t:pagelink> 
			    				<br /><br />
			    			</div>
			    			
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/CircleGraph" context="${CurrentCourse?.id}"><img src="${circleGraphIcon}" height="50px" width="60px" /><br /><br /><b>Circle Graph [ALPHA]</b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			
			    			<div class="span2" align="center">
			    				<t:pagelink page="viz/visualizationHeatmap" context="${CurrentCourse?.id}"><img src="${heatMapAnalysisIcon}" height="50px" width="60px" /><br /><br /><b>Heatmap [ALPHA]</b> </t:pagelink>
			    				<br /><br />
			    			</div>
			    			
			    			
<!--		    				<div class="span2" align="center">-->
<!--			    				<t:pagelink page="data/visualization" context="${CurrentCourse.courseid}"><img src="${usageAnalysisIcon}" height="50px" width="85px" /><br /><br /><b>${message:usageAnalysisTime} (OLD) </b> </t:pagelink>-->
<!--			    				<br /><br />-->
<!--			    			</div>-->
<!--			    			<div class="span2" align="center">-->
<!--			    				<t:pagelink page="data/visualizationLO" context="${CurrentCourse.courseid}"><img src="${usageAnalysisLOIcon}" height="50px" width="75px" /><br /><br /><b>${message:usageAnalysisLO} (OLD)</b> </t:pagelink> -->
<!--			    				<br /><br />-->
<!--			    			</div>-->
		    				<div class="span4">
		    				</div>
		    			</div>
		    		</t:zone> <!-- analysisSelectZone  -->		
    			</div>
    		</div>
    	</div> <!-- span9 -->
    	
    	
    </div>

