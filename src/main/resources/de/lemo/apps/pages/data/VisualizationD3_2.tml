<html t:type="layoutdashboard" title="LeMo | Visualization"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
      
        <style type="text/css">

path.link {
  fill: none;
  stroke: #666;
  stroke-width: 1.5px;
}

marker#click {
  fill: blue;
}

path.link.click {
  stroke: blue;
}

path.link.resolved {
  stroke-dasharray: 0,2 1;
}

circle {
  fill: #ccc;
  stroke: #333;
  stroke-width: 1.5px;
}

text {
  font: 10px sans-serif;
  pointer-events: none;
}

text.shadow {
  stroke: #fff;
  stroke-width: 3px;
  stroke-opacity: .8;
}

    </style>
     
     <!-- Breadcrumb component-->
	<div class="row">
        <div class="span12">
    	<ul class="breadcrumb">
  			<li t:type="crumb/breadcrumbtrail" t:value="breadCrumb">
    			<a t:type="crumb/displaybreadcrumb" t:breadcrumb="breadCrumb" />
  			</li>
		</ul>
		</div>
	</div>
	
	<div class="row-fluid">
		
    	
    	
    	
    	<div class="span9">
    			 <div id="viz"></div>
    	</div> <!-- span9 -->
    	
    	
    	<div class="span3">
    		<div class="row-fluid">
    		<div class="span12">
    			<div class="well">
    				<h4>${course.courseDescription}</h4>
    				<br />
    				${message:available}: ${FirstRequestDate} - ${LastRequestDate}
    				<br />
    				<br />
    				${message:range}: ${resolution} ${message:days}
    			</div>
    		</div>
    		</div>
    		<div class="row-fluid">
    		<div class="span12">
<!--			<div class="thumbnail">-->
        		<form class="form-search">
        		<div class="">
        			
        			<t:zone t:id="formZone" id="formZone">
        			<form t:type="form" t:id="customizeform" class="well  pull-left">
<!--				    	-->
<!--				    <h4>${message:options}</h4>-->
<!--				    <br />-->
        			
        			<div class="accordion" id="accordion2">
			            <div class="accordion-group">
			              <div class="accordion-heading">
			                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseOne">
			                  ${message:dateTimeOption} <b class="caret"></b>
			                </a>
			              </div>
			              <div style="height: auto;" id="collapseOne" class="accordion-body in collapse">
			                <div class="accordion-inner">
			                
			                	<br />
						    	${message:begin}:&nbsp;&nbsp;
						    	<t:datefield class="input-small" t:value="beginDate" t:mixins="jquery/CustomDatepicker" t:params="datepickerparams"/>
						    	<br />
						    	<br />
						    	${message:end}:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
						    	<t:datefield class="input-small" t:value="endDate"  t:mixins="jquery/CustomDatepicker" t:params="datepickerparams"/>
						    	<br />
			                </div>
			              </div>
			            </div>
			            <div class="accordion-group">
			              <div class="accordion-heading">
			                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseTwo">
			                  ${message:activityOption} <b class="caret"></b>
			                </a>
			              </div>
			              <div style="height: 0px;" id="collapseTwo" class="accordion-body collapse">
			                <div class="accordion-inner">
			                 	<br />
						  
		            			<t:jquery.palette t:id="activities" encoder="activityEncoder" model="activityModel"/>
			                </div>
			              </div>
			            </div>
			            <div class="accordion-group">
			              <div class="accordion-heading">
			                <a class="accordion-toggle dropdown-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseThree">
			                 Member <b class="caret"></b>
			                </a>
			              </div>
			              <div style="height: 0px;" id="collapseThree" class="accordion-body collapse">
			                <div class="accordion-inner">
<!--			                 	<t:jquery.palette t:id="courses" encoder="courseValueEncoder" model="courseModel"/>	-->
			                </div>
			              </div>
			            </div>
			          </div>
        			
				    	
				    	
<!--				    <br/>
				    	<t:jquery.slider t:value="val" t:params="params"/>
				    	  <t:jquery.slider t:value="slideZone" t:params="paramsZone" t:zone="myZone" t:id="sliderZone" class="ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all" />
						<br/>
						<t:zone t:id="myZone" id="myZone">
						Slider Value: ${slideZone}
						</t:zone>-->
				    	
				    	
			    	 	<!--	<t:actionlink t:id="update" class="btn btn-primary pull-right">update</t:actionlink>-->
			    	 	
				    	
				   		<input t:type="submit" value="Update" class="btn btn-primary pull-left" />
		  				<br />
				    	
				   </form>	
				   </t:zone> 
        			
<!--    				<input type="text" class="input-large search-query"/>-->
<!--    				<button type="submit" class="btn pull-right">${message:search}</button>-->
    			</div>
    			</form>
<!--    		</div> THUMBNAIL-->
    		</div> <!--span12-->
    		</div> <!--row fluid-->
    	</div>
    	
    
   </div>
    <script type="text/javascript">



	
	

	
    </script>
    
</html>